	<ItemGroup>
	    <EmbeddedResource Include="Radar\Maps\*" />
	</ItemGroup>
    <ItemGroup>
        <PackageReference Include="SkiaSharp" Version="3.119.1" />
		<PackageReference Include="ECommons" Version="3.1.0.1" />
    </ItemGroup>
</Project>